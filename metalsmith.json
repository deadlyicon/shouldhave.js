{
  "source": "./docs/src",
  "destination": "gh-pages",
  "metadata": {
    "title": "shouldhave.js",
    "description": "Everything JavaScript should have :D"
  },
  "plugins": {
    "metalsmith-ignore": [
      "./docs/templates/*",
      "./docs/styles/*",
    ]
    "metalsmith-sass": {
      "outputStyle": "expanded"
    },
    "metalsmith-markdown": {},
    "metalsmith-code-highlight": {},
    "metalsmith-templates": {
      "engine": "jade",
      "directory": "./docs/templates"
    }
  }
}
