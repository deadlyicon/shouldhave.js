{
  "source": "./docs",
  "destination": "gh-pages",
  "clean": false,
  "ignore": [
    "_templates/*"
  ],
  "metadata": {
    "title": "shouldhave.js",
    "description": "Everything JavaScript should have :D"
  },
  "plugins": {
    "metalsmith-ignore": [
      "_templates/*"
    ],
    "metalsmith-sass": {
      "outputStyle": "expanded"
    },
    "metalsmith-markdown": {},
    "metalsmith-code-highlight": {},
    "metalsmith-templates": {
      "engine": "jade",
      "directory": "./docs/_templates"
    }
  }
}
